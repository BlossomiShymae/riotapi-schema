{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "title": "FeaturedGameInfo",
  "properties": {
    "gameId": {
      "type": "integer",
      "format": "int64",
      "_type": "long",
      "description": "The ID of the game"
    },
    "gameStartTime": {
      "type": "integer",
      "format": "int64",
      "_type": "long",
      "description": "The game start time represented in epoch milliseconds"
    },
    "platformId": {
      "type": "string",
      "_type": "string",
      "description": "The ID of the platform on which the game is being played"
    },
    "gameMode": {
      "type": "string",
      "_type": "string",
      "description": "The game mode"
    },
    "mapId": {
      "type": "integer",
      "format": "int64",
      "_type": "long",
      "description": "The ID of the map"
    },
    "gameType": {
      "type": "string",
      "_type": "string",
      "description": "The game type"
    },
    "bannedChampions": {
      "type": "array",
      "items": {
        "$ref": "./BannedChampion.json",
        "_type": "BannedChampion"
      },
      "_type": "List[BannedChampion]",
      "description": "Banned champion information"
    },
    "observers": {
      "$ref": "./Observer.json",
      "_type": "Observer",
      "description": "The observer information"
    },
    "participants": {
      "type": "array",
      "items": {
        "$ref": "./Participant.json",
        "_type": "Participant"
      },
      "_type": "List[Participant]",
      "description": "The participant information"
    },
    "gameLength": {
      "type": "integer",
      "format": "int64",
      "_type": "long",
      "description": "The amount of time in seconds that has passed since the game started"
    },
    "gameQueueConfigId": {
      "type": "integer",
      "format": "int64",
      "_type": "long",
      "description": "The queue type (queue types are documented on the Game Constants page)"
    }
  }
}